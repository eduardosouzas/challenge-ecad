{
  "name": "front-end-ecad",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "@coreui/coreui": "^2.1.16",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.3.1",
    "@coreui/icons": "^1.0.0",
    "@coreui/react": "^2.5.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.9.12",
    "axios": "^0.19.2",
    "flag-icon-css": "^3.0.0",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "react-bootstrap": "^0.32.1",
    "react-calendar": "^2.17.5",
    "react-chartjs-2": "^2.7.2",
    "react-clear-cache": "^1.2.0",
    "react-content-loader": "^5.0.4",
    "react-loadable": "^5.5.0",
    "react-loading": "^2.0.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0",
    "react-table": "6.8.6",
    "react-table-6": "^6.11.0",
    "react-test-renderer": "^16.13.1",
    "react-toastify": "^4.1.0",
    "react-tooltip": "^4.2.5",
    "reactstrap": "8.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "reload": "^3.0.4",
    "simple-line-icons": "^2.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.19.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test a --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "start": "set REACT_APP_STAGE='LOCAL' && npm-run-all -p watch-css start-js",
    "build": "set REACT_APP_STAGE='LOCAL' && npm-run-all build-css build-js",
    "start:dev": "set REACT_APP_STAGE='DEV' && npm-run-all -p watch-css start-js",
    "build:dev": "set REACT_APP_STAGE='DEV' && npm-run-all build-css build-js && npm run postbuild",
    "start:homol": "set REACT_APP_STAGE='HML' && npm-run-all -p watch-css start-js",
    "build:homol": "set REACT_APP_STAGE='HML' && npm-run-all build-css build-js && npm run postbuild",
    "start:prod": "set REACT_APP_STAGE='PROD' && npm-run-all -p watch-css start-js",
    "build:prod": "set REACT_APP_STAGE='PROD' && npm-run-all build-css build-js && npm run postbuild",
    "postbuild": "node ./postbuild.js",
    "lint": "eslint src/** --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
